	<insert id="createTb">
		insert into cur_reserve (parent_board_seq, cur_date) values(#{parent_board_seq},#{cur_date})
	</insert>
	
	<delete id="deleteBoard">
		delete from petsitter_board where psb_seq=#{psb_seq}
	</delete>
	
